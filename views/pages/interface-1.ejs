<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title><%= title %></title>
  <link href="/css/styles.css" rel="stylesheet" type="text/css">
  <meta name="viewport" content="width=device-width,initial-scale=1">
</head>
<body>
  <div class="interface-wrapper">
    <h1><%= title %></h1>
    <div class="controls-wrapper">
      <div class="image-wrapper">
        <div class="control-wrapper">
          <img id="add-icon" class="control" src="/image/hdpi/ic_add.png" onClick='javascript:serverRequest("/add")'>
        </div>
        <div class="control-wrapper">
          <img id="edit-icon" class="control" src="/image/hdpi/ic_edit.png" onClick='javascript:serverRequest("/edit")'>
        </div>
        <div class="control-wrapper">
          <img id="delete-icon" class="control" src="/image/hdpi/ic_delete.png">
        </div>
      </div>
      <div class="input-wrapper">
        <input type="text" id="field-input" class="field-input" maxlength="25" placeholder="Enter product name">
      </div>
    </div>
    <div class="button-list-wrapper">
      <% items.forEach(function(r) {%>
        <div class="button-wrapper">
          <button type="button" class="button list-button" id='<%= r.id %>'><%= r.name %></button>
        </div>
      <% }); %>
    </div>
  </div>

  <!-- JQuery Library -->
  <script
  src="https://code.jquery.com/jquery-3.6.0.min.js"
  integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4="
  crossorigin="anonymous"></script>
  <!-- Javascript Files -->
  <script src="/js/common.js"></script>
  <!-- <script src="/js/InterfaceButton.js"></script>
  <script src="/js/InterfaceList.js"></script>
  <script src="<%= jsfile %>"></script> -->
</body>
</html>
